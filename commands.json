  "Presets": {
    "kill": {
      "description": "Kills the target's character.",
      "steps": [
        "setprop target.Character.Humanoid.Health 0"
      ]
    },
    "heal": {
      "description": "Heals the target's character.",
      "steps": [
        "run target.Character.Humanoid TakeDamage -1000000"
      ]
    },
    "speed": {
      "description": "Changes the target's walkspeed.",
      "steps": [
        "setprop target.Character.Humanoid.WalkSpeed $1"
      ]
    },
    "bring": {
      "description": "Teleports the target to the executor.",
      "steps": [
        "run target.Character PivotTo executor_pivot"
      ]
    },
    "m": {
      "description": "Sends a global server message.",
      "steps": [
        "hint all [SERVER]: $1"
      ]
    },
    "fling": {
      "description": "Y-axis launch using AssemblyLinearVelocity (no attachments needed).",
      "steps": [
        // ➊ stop any momentum so the fling is consistent
        "setprop target.Character.HumanoidRootPart.AssemblyLinearVelocity Vector3(0,0,0)",
        // ➋ give the punch
        "setprop target.Character.HumanoidRootPart.AssemblyLinearVelocity Vector3(0,120,0)"
      ]
    },
    
    "shutdown": {
      "description": "Kicks everyone with a single-argument reason.",
      "steps": [
        "runonall game.Players.GetPlayers Kick \"Server is shutting down.\""
      ]
    }
  }
}
